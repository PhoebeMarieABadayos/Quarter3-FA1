function myFunction() {
  
  var nick = "Phoebe";
  var height = "60.2 inches";
  var weight = "51 kg"

alert ("Name: " + nick + "Height: " + height + "Weight: " + weight);
  var txt;
  if (confirm("Do you wish to share your personal information with the site?")) {
    txt = console.log("Name: " + nick + "Height: " + height + "Weight: " + weight);
  } else {
    txt = console.log("User does not wish to share his/her information");
  }
  document.getElementById("demo").innerHTML = txt;
}

